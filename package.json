    "@parcel/transformer-pug": "^2.0.1",
    "@parcel/transformer-webmanifest": "^2.0.1",
    "@parcel/transformer-xml": "^2.0.1",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.25.3",
    "markdown-it": "^11.0.1",
    "pug": "^3.0.0"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "term-img": "^5.0.0",
    "terminal-link": "^2.1.1"
  }
