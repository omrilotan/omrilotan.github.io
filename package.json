  "repository": {
    "type": "git",
    "url": "git+https://github.com/omrilotan/omrilotan.github.io.git"
  },
  "scripts": {
    "prestart": "which parcel || npm i parcel --no-save",
    "start": "parcel src/index.pug --dist-dir docs",
